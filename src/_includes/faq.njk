---
layout: base.njk
---

{{ content | safe }}


<div class="fr-accordions-group">
  {% for question in collections.faq %}
  <section class="fr-accordion">
    <h3 class="fr-accordion__title">
      <button
        class="fr-accordion__btn"
        aria-expanded="false"
        aria-controls="accordion-{{ loop.index }}"
      >
        {{ question.data.title }}
      </button>
    </h3>
    <div class="fr-collapse" id="accordion-{{ loop.index }}">{{ question.templateContent | safe }}</div>
  </section>
  {% endfor %}
</div>
